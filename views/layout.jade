doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name="description" content="Technology first creative studio. Experienced VR, AR, installations and activation developers. We specialise in experimental technology which intersects art.")
    meta(name="keywords" content="VR, AR, virtual reality, oculus, vive, technology, machine learning, installations, activation")
    meta(name="author" content="Unsigned Studio PTY LTD")
    title Unsigned Studio | Melbourne Based | Creative Technology, Data Visualization, VR, AR, Visual Experiences, Activations and Installations
    link(href='css/bootstrap.min.css', rel='stylesheet')
    link(href='css/style.css', rel='stylesheet')
    link(href='css/xs.css', rel='stylesheet')
    link(href='css/sm.css', rel='stylesheet')
    link(rel='icon', type='image/png', href='images/favicon.ico')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
